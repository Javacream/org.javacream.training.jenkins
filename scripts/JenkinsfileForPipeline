pipeline {
    agent any
    tools {
        maven 'Maven3'
        jdk 'Java8'
    }
    stages {
        stage ('Initialize') {
            steps {
                git 'https://github.com/Javacream/org.javacream.training.jenkins.git'
            }
        }

        stage ('Build') {
            steps {
                sh 'mvn  -f org.javacream.training.jee7/pom.xml install' 
            }
        }
    }
}
